      "import": "./dist/main.js"
    }
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "volta": {
    "node": "22.12.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
