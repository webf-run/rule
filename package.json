{
  "name": "@webf/rule",
  "version": "1.0.1",
  "description": "Business rule validation library",
  "keywords": [
    "rule",
    "validation",
    "business"
  ],
  "author": {
    "name": "Harshal Patil"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webf-run/rule.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check:types": "tsc -p tsconfig.json",
    "dev:format": "prettier --write ."
